    "file-loader": "^0.11.1",
    "flow-bin": "^0.48.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cli": "^1.3.0",
    "gulp-sourcemaps": "^2.6.0",
    "node-sass": "^4.5.3",
    "promisify-node": "^0.4.0",
    "require-dir": "^0.3.2",
