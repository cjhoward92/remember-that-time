{
  "name": "remember-that-time",
  "version": "1.0.0",
  "description": "A fun app for organizing photos",
  "main": "src/main.js",
  "scripts": {
    "build": "npm run flow && cross-env NODE_ENV=prod ./node_modules/.bin/webpack --config webpack.config.js --display-error-details",
    "start": "npm run build && npm run electron",
    "dev": "cross-env NODE_ENV=dev HOT=1 START_HOT=1 ./node_modules/.bin/webpack-dev-server --config webpack.config.js",
    "electron": "electron .",
    "start-hot-renderer": "cross-env HOT=1 NODE_ENV=development electron .",
    "flow": "\"./node_modules/.bin/flow\" check",
    "bootstrap": "git clean -xdff && yarn cache clean && npm i",
    "lint": "\"./node_modules/.bin/eslint\" src/"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [],
  "author": "Carson Howard",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "electron": "~1.6.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-flow": "^1.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.48.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.2.1",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "webpack": "^2.6.1"
  }
}
