{
  "name": "remember-that-time",
  "version": "1.0.0",
  "description": "A fun app for organizing photos",
  "main": "dist/main.js",
  "scripts": {
    "compile": "\"./node_modules/.bin/webpack\" --config webpack.config.js --display-error-details",
    "build": "npm run flow && cross-env NODE_ENV=prod && yarn compile",
    "start": "npm run build && npm run electron",
    "dev": "cross-env NODE_ENV=dev && yarn compile && yarn electron",
    "electron": "electron .",
    "flow": "\"./node_modules/.bin/flow\" check",
    "bootstrap": "git clean -xdff && yarn cache clean && npm i",
    "lint": "\"./node_modules/.bin/eslint\" src/",
    "babel": "\"./node_modules/.bin/babel\" src --out-dir dist --source-maps",
    "gulp": "\"./node_modules/.bin/gulp\""
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [],
  "author": "Carson Howard",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "electron": "~1.6.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.2.0",
